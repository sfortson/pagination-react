{
  "name": "react-bulma-io-components",
  "version": "0.3.2",
  "description": "React components using Bulma CSS framework",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest src --passWithNoTests",
    "test:watch": "jest src --watch --notify",
    "cover": "jest src --coverage",
    "lint": "eslint src",
    "build": "yarn build:rollup && yarn build:types",
    "build:rollup": "rollup -c",
    "build:types": "tsc -d --emitDeclarationOnly --declarationDir lib",
    "prepublishOnly": "yarn clean && yarn lint && yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfortson/react-bulma.git"
  },
  "keywords": [
    "react",
    "bulma"
  ],
  "author": "Sam Fortson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfortson/react-bulma/issues"
  },
  "homepage": "https://github.com/sfortson/react-bulma#readme",
  "dependencies": {
    "bulma": "^0.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^4.17.0",
    "@yarnpkg/pnpify": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "latest",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "resolve": "^1.19.0",
    "rollup": "2.34.0",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-includepaths": "^0.2.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "webpack": "^5.10.0"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
